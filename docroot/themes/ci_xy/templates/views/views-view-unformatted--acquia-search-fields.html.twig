{% if title %}
  <h3>{{ title }}</h3>
{% endif %}

<ul>
    {% for row in rows %}
        {%
        set row_classes = [
        default_row_class ? 'views-row searchSolr promoted-search',
        ]
        %}
      <li{{ row.attributes.addClass(row_classes) }}>
          {{ row.content }}
      </li>
    {% endfor %}
</ul>

